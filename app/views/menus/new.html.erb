<div id="contact" class="big-bg">
  <div class="wrapper">
    <h2 class="page-title">Entry new Menu</h2>
        <%= form_with(model: @menu, local: true) do |f| %>
          <%= f.label :name %>
          <%= f.text_field :name, class: 'form-control' %>
          <%= f.label :comment %>
          <%= f.text_area :comment, class: 'form-control' %><BR>
          <%= f.label :photo %>
          <%= f.file_field :image %><BR>
          <%= f.submit "Create new menu", class: "button-create" %>
        <% end %>
    </div>
  </div><!-- /.wrapper -->
</div><!-- /#contact -->
